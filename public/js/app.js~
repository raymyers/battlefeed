google.load("jquery", "1.4.2");
google.load("jqueryui", "1.8.3");
google.setOnLoadCallback(function() {
    $(function() {
        $.getJSON("http://gdata.youtube.com/feeds/api/users/drect/uploads?v=2&alt=jsonc&max-results=25&start-index=1", gotted);
        $("ul.vids li").live('click', function() {
            $(".details").text($(this).data('item').description);
        });
        $("ul.vids").selectable();
    });
});
function gotted(data,textStatus) {
    var total = data.data.totalItems;
    $.each(data.data.items, function(i, item) {
        $("ul.vids").append("<li>" + item.title + "</li>");
        $("ul.vids li:last").data('item', item);
    });
}